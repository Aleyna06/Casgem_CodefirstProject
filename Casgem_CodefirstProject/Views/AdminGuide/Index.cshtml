@model List<Casgem_CodefirstProject.DAL.Entities.Guide>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Admin/Index.cshtml";
}

<h2>Rehber İşlemi</h2>
<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Rehber Adı</th>
        <th>Unvan</th>
        <th>Sil</th>
        <th>Güncelle</th>
    </tr>
    @foreach (var item in Model)
    {
<tr>
    <td>@item.GuideID</td>
    <td>@item.GuideName</td>
    <td>@item.GuideTitle</td>
    <td><a href="/AdminGuide/DeleteGuide/@item.GuideID" class="btn btn-outline-danger">Sil</td>
    <td><a href="/AdminGuide/UpdateGuide/@item.GuideID" class="btn btn-outline-success">Güncelle</td>
</tr>            }

</table>
<a href="#" class="btn btn-outline-info">Yeni Rehber Girişi </a>